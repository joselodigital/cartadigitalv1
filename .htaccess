RewriteEngine On
RewriteBase /catalogodigital/

# Prevent directory listing
Options -Indexes

# If the request is not for a real file or directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite simple slugs to the catalog view
RewriteRule ^([a-zA-Z0-9-]+)/?$ index.php?view=catalog&slug=$1 [L,QSA]
